<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for sale.order -->
        <record id="seq_school_student" model="ir.sequence">
            <field name="name">student sequence</field>
            <field name="code">school.student.sequence</field>
            <field name="prefix">%(y)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_school_department" model="ir.sequence">
            <field name="name">department sequence</field>
            <field name="code">school.department.sequence</field>
            <field name="prefix">DT</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_school_subject" model="ir.sequence">
            <field name="name">subject sequence</field>
            <field name="code">school.subject.sequence</field>
            <field name="prefix">ST</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
